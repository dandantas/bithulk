# Server config
PORT=3000
NODE_ENV=development
LOG_LEVEL=info

# Bitbucket API
BITBUCKET_API_URL=https://api.bitbucket.org/2.0
BITBUCKET_WORKSPACE_ID=your_bitbucket_workspace_id
BITBUCKET_ACCESS_TOKEN=your_bitbucket_access_token

# AI Provider Selection
# Options: openai, deepseek, local
AI_PROVIDER_TYPE=deepseek

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_API_URL=https://api.openai.com/v1/chat/completions
OPENAI_MODEL=gpt-4-turbo-preview

# DeepSeek API Configuration (using local Ollama)
DEEPSEEK_API_URL=http://ollama:11434/api/generate
DEEPSEEK_MODEL=deepseek-r1:14b

# Google Chat API
GOOGLE_CHAT_WEBHOOK_URL=your_google_chat_webhook_url 